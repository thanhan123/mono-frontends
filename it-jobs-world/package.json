    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.8.0",
    "lint-staged": "^13.1.0",
    "prettier": "2.8.1",
    "sass": "^1.56.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.0",
    "vite-plugin-pwa": "^0.14.0",
    "vue-tsc": "^1.0.9"
  }
